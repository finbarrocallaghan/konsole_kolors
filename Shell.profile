[Appearance]
AntiAliasFonts=true
BoldIntense=false
ColorScheme=pytest
Font=Ubuntu Mono,9,-1,5,50,0,0,0,0,0

[Cursor Options]
CursorShape=0
CustomCursorColor=130,200,150
UseCustomCursorColor=true

[General]
Command=/bin/zsh
Icon=utilities-terminal
LocalTabTitleFormat=%D : %n
Name=Shell
Parent=FALLBACK/
RemoteTabTitleFormat=(%u) %H
ShowMenuBar=false
TabBarMode=0

[Scrolling]
ScrollBarPosition=2

[Terminal Features]
BidiRenderingEnabled=true
